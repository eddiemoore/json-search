{
  "name": "zdsearch",
  "version": "1.0.0",
  "description": "Search data in JSON files",
  "main": "./lib/search.js",
  "bin": {
    "zdsearch": "index.js"
  },
  "scripts": {
    "precommit": "npm run lint && lint-staged",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "search",
    "json"
  ],
  "author": "Ed Moore <eddie.moore@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eddiemoore/json-search"
  },
  "bugs": {
    "url": "https://github.com/eddiemoore/json-search/issues"
  },
  "homepage": "https://github.com/eddiemoore/json-search",
  "dependencies": {
    "as-table": "1.0.36",
    "chalk": "2.4.1",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "eslint": "7.15.0",
    "eslint-config-prettier": "3.0.1",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lint-staged": "7.2.2",
    "prettier": "1.14.2"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "lint-staged": {
    "**/*.{js,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/data/**"
    ]
  }
}
